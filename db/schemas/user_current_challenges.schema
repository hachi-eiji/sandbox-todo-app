# -*- mode: ruby -*-
# vi: set ft=ruby :
create_table "user_current_challenges", options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4", force: :cascade do |t|
  t.bigint "user_id", null: false
  t.string "current_challenges", null: false
  t.datetime "created_at", null: false
  t.datetime "updated_at", null: false
  t.index ["user_id", "current_challenges"], name: "user_id_2"
  t.index ["user_id"], name: "user_id", unique: true
end
